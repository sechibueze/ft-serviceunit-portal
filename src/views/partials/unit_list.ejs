<div class="">

  <% if(result.data.length > 0){
    let count = 0 %>
  <!-- Put up table -->
  <table class="table">
    <thead>
      <tr>
        <th>S/N</th>
        <th>Unit_ID</th>
        <th>Unit</th>
        <th>Unit Username</th>
        <th>Unit Password</th>
        <th>Delete</th>
      </tr>
    </thead>
    <tbody>
      <% result.data.forEach(record => {
        %>
      <tr>
        <td><%= ++count %></td>
        <td><%= record.unit_id %></td>
        <td><%= record.unit %></td>
        <td><%= record.unit_username %></td>
        <td><%= record.unit_password %></td>
        <td><a href="/admin/units/<%= record.unit_id %>/delete">Delete</a></td>
      </tr>
      <%})%>
    </tbody>

  </table>
  <% }%>

</div>